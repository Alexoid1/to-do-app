// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./taskmcd.db"
}

model task {
  id Int @id @default(autoincrement())
  title String
  deadLine Int
  startTime Int
  endTime Int
  remind Int
  repeat Int
  status Int

}